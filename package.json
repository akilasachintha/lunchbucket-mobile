{
  "name": "lunchbucket",
  "version": "1.2.0",
  "scripts": {
    "hello": "echo $NODE_ENV",
    "start": "expo start --dev-client",
    "dev": "cross-env NODE_ENV=development expo start -c",
    "prod": "cross-env NODE_ENV=production expo start -c",
    "test": "cross-env NODE_ENV=test&& expo start -c",
    "build:dev:apk": "cross-env NODE_ENV=development eas build --platform android --profile development --local",
    "build:dev:ios": "cross-env NODE_ENV=development eas build --platform ios --profile development --local",
    "build:prod:aab": "cross-env NODE_ENV=production eas build --platform android --profile production --local",
    "build:prod:apk": "cross-env NODE_ENV=production eas build --platform android --profile productionApk --local",
    "build:prod:ios": "cross-env NODE_ENV=production eas build --platform ios --profile production --local",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/native-stack": "6.9.13",
    "@reduxjs/toolkit": "1.9.5",
    "axios": "1.6.0",
    "expo": "~50.0.8",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-env": "1.1.1",
    "expo-font": "~11.10.3",
    "expo-network": "~5.8.0",
    "expo-notifications": "~0.27.6",
    "expo-status-bar": "1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.11",
    "formik": "2.4.4",
    "moment": "2.29.4",
    "moment-timezone": "^0.5.44",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-swiper": "1.6.0",
    "react-native-vector-icons": "10.0.0",
    "react-redux": "8.1.2",
    "redux-thunk": "2.4.2",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.19",
    "@types/react": "~18.2.45",
    "cross-env": "7.0.3",
    "typescript": "^5.1.3"
  },
  "private": true
}
